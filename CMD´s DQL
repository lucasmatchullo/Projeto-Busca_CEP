

select e.sigla as 'Estado', c.nome as 'Cidade', c.cep as 'CEP Geral', b.nome as 'Bairro', l.nome as 'Logradouro', l.cep, l.complemento from estados as e inner join cidades as c on e.sigla=c.estado_sigla inner join bairros as b on  c.id=cidade_id left join logradouros as l on b.id=l.bairro_id;
select e.sigla as 'Estado', c.nome as 'Cidade', c.cep as 'CEP Geral', b.nome as 'Bairro', l.nome as 'Logradouro', l.cep, l.complemento from estados as e left join cidades as c on e.sigla=c.estado_sigla left join bairros as b on  c.id=cidade_id left join logradouros as l on b.id=l.bairro_id;


 
select e.sigla as 'Sigla', c.nome as 'Cidade', c.cep, b.nome as 'Bairro', l.cep as 'CEP', l.nome as 'Logradouro', l.complemento from estados as e inner join cidades as c on e.sigla=c.estado_sigla inner join bairros as b on  c.id=cidade_id inner join logradouros as l on b.id=l.bairro_id where l.nome=NULL or l.cep like '45028-904%';

select e.sigla as 'Sigla', c.nome as 'Cidade', c.cep, b.nome as 'Bairro', l.cep as 'CEP', l.nome as 'Logradouro', l.complemento from estados as e inner join cidades as c on e.sigla=c.estado_sigla inner join bairros as b on  c.id=cidade_id inner join logradouros as l on b.id=l.bairro_id where l.nome like '%Av Brasil%' or l.cep=NULL;

select e.sigla as 'Sigla', c.nome as 'Cidade', c.cep as 'CEP Geral', b.nome as 'Bairro', l.nome as 'Logradouro' from estados as e left join cidades as c on e.sigla=c.estado_sigla left join bairros as b on  c.id=cidade_id left join logradouros as l on b.id=l.bairro_id where l.cep like=45028-904' or upper (l.mome) like upper (%45028-904) ;
